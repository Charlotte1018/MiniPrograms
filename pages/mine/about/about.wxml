<!--about.wxml-->
<import src="../../../common/tpl.wxml" />
<view class="about_title">
  <view class="">
    <view bindtap="" class="center">
      <image class="logo" src="../../../image/etaibao.png"></image>
    </view>
    <view class="mt center">
      <text>版本：{{appVersion}} </text>
    </view>
  </view>
</view>

<view class="box mt bg">
  <view class="" bindtap="version">
    <view class="navigator">
      <text class="navigator-text" style="margin-left:40rpx;">版本说明</text>
      <view class="navigator-arrow"></view>
    </view>
  </view>
  <view class="cell" bindtap="privacyolicy">
    <view class="navigator">
      <text class="navigator-text" style="margin-left:40rpx;">隐私权政策</text>
      <view class="navigator-arrow"></view>
    </view>
  </view>
  <view class="cell" bindtap="useAgreement">
    <view class="navigator">
      <text class="navigator-text" style="margin-left:40rpx;">使用协议</text>
      <view class="navigator-arrow"></view>
    </view>
  </view>
  <view class="cell" bindtap="">
    <view class="navigator">
      <text class="navigator-text" style="margin-left:40rpx;">检测新版本</text>
      <view class="navigator-arrow"></view>
    </view>
  </view>
</view>

<view class="bg mt">
  <view class="navigator" bindtap="aboutWe">
    <text class="navigator-text" style="margin-left:40rpx;">关于我们</text>
    <view class="navigator-arrow"></view>
  </view>

</view>