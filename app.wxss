/**app.wxss**/
@import "./common/common.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 0;
  box-sizing: border-box;
} 
.header{
  width:750rpx;
  height:300rpx;
  background-color: white;
}
.logo{
  width:100rpx;
  height:100rpx;
}
.position{
  display: inline-block;
  text-align: center;
  margin-top: 60rpx;
  margin-left: 110rpx;
  margin-right: 10rpx;
}
.cardList{
  width:710rpx;
  margin-left: 20rpx;
}
.inline{
  display: inline-block;
  text-align: center;
}
.txt{
  color: grey;
}
.fr{
  margin-left: 400rpx;
}
.btn_m{
  width: 25%;
}
.hidden {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.center{
  text-align: center;
}
.btnclass{
  color: #3399ff; border-color: #3399ff;
}
.btnhover{
  color: white; background-color: #3399ff;border-bottom-style:outset;border-bottom-color:#F0F8FF;
}
